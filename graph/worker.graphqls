extend type Query {
  worker: [String!]!  @AuthCheck(role: ADMIN)
}

extend type Mutation {
  cluster_register(input: String!): String! @AuthCheck(role: ADMIN)
  cluster_mortgage(input: String!): String! @AuthCheck(role: ADMIN)
  cluster_unmortgage(input: String!): String! @AuthCheck(role: ADMIN)
  cluster_withdrawal(input: String!): String! @AuthCheck(role: ADMIN)
  cluster_stop(input: String!): String! @AuthCheck(role: ADMIN)
}
